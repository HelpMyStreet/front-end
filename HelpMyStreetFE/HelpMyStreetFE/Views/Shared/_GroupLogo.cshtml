@using HelpMyStreet.Utils.Enums;

@model HelpMyStreetFE.Models.Community.GroupLogoViewModel

<div class="group-logo">
    @switch (Model.GroupId)
    {
        case (int)Groups.Generic:
            <span class="composite-logo">
                <img src="~/img/logos/HelpMyStreet_BrandMarque_Transparent.svg" alt="HelpMyStreet" />
                <span>HelpMyStreet.org</span>
            </span>
            break;
        case (int)Groups.AgeUKWirral:
            <img src="~/img/community/ageUK/wirral/AgeUKWirral-logo.png" alt="AgeUK Wirral" />
            break;
        case (int)Groups.AgeUKLSL:
            <img src="~/img/community/ageUK/lsl/AgeUKLSL_logo_300px.png" alt="AgeUK Lincoln & South Lincolnshire" />
            break;
        case (int)Groups.Ruddington:
            <span class="composite-logo">
                <img src="~/img/logos/HelpMyStreet_BrandMarque_Transparent.svg" alt="HelpMyStreet" />
                <span>Ruddington</span>
            </span>
            break;
        case (int)Groups.Tankersley:
            <span class="composite-logo">
                <img src="~/img/logos/HelpMyStreet_BrandMarque_Transparent.svg" alt="HelpMyStreet" />
                <span>Tankersley</span>
            </span>
            break;
        case (int)Groups.AgeUKNottsNorthMuskham:
            <span class="composite-logo">
                <img src="~/img/logos/HelpMyStreet_BrandMarque_Transparent.svg" alt="HelpMyStreet" />
                <span>North Muskham</span>
            </span>
            break;
        case (int)Groups.FTLOS:
            <img src="~/img/community/fortheloveofscrubs/FTLOS.png" alt="For the Love of Scrubs" />
            break;
        case (int)Groups.AgeUKNottsBalderton:
            <span class="composite-logo">
                <img src="~/img/logos/HelpMyStreet_BrandMarque_Transparent.svg" alt="HelpMyStreet" />
                <span>Balderton</span>
            </span>
            break;
        case (int)Groups.HLP:
            <img src="~/img/community/hlp/MHFA-England-logo.png" alt="MHFA England" />
            break;
        case (int)Groups.AgeUKFavershamAndSittingbourne:
            <img src="~/img/community/ageUK/kent/favershamandsittingbourne/logo.png" alt="Age UK Faversham & Sittingbourne" />
            break;
        case (int)Groups.AgeUKNorthWestKent:
            <img src="~/img/community/ageUK/kent/northwest/logo.png" alt="Age UK Northwest Kent" />
            break;
        case (int)Groups.AgeUKSouthKentCoast:
            <img src="~/img/community/ageUK/kent/southkentcoast/logo.png" alt="Age UK South Kent Coast" />
            break;
        case (int)Groups.LincolnPCN:
            <img src="~/img/community/vacc/lincolnshirevolunteers/APEX-PCN.png" alt="APEX" />
            break;
        case (int)Groups.StamfordPCN:
            <img src="~/img/community/vacc/lincolnshirevolunteers/four-counties-logo.png" alt="Four Counties PCN" />
            break;
        case (int)Groups.LincolnPortlandPCN:
        case (int)Groups.LincolnshireVolunteers:
        case (int)Groups.LouthPCN:
        case (int)Groups.SouthLincolnPCN:
        case (int)Groups.SpilsbyPCN:
        case (int)Groups.GranthamPCN:
        case (int)Groups.BostonPCN:
            <span class="composite-logo">
                <img src="~/img/logos/HelpMyStreet_BrandMarque_Transparent.svg" alt="HelpMyStreet" />
                <span>Lincs Volunteers</span>
            </span>
            break;
        case (int)Groups.MansfieldCVS:
            <img src="~/img/community/vacc/mansfield-cvs/MansfieldCVS.jpg" alt="Mansfield CVS" />

            break;
        case (int)Groups.MansfieldWickes:
        case (int)Groups.GamstonCommunityHall:
        case (int)Groups.RichardHerrodCentre:
        case (int)Groups.KingsMeadowCampus:
        case (int)Groups.ForestRecreationGround:
            <img src="~/img/community/vacc/mansfield-cvs/nottinghamshire-vaccine-volunteers.jpg" alt="Nottinghamshire Vaccine Volunteers" />
            break;
        case (int)Groups.Sandbox:
            <img src="~/img/logos/sandbox.svg" alt="HMS Sandbox" />
            break;
        case (int)Groups.AgeConnectsCardiff:
            <img src="~/img/community/ageconnectscardiff/logo.png" alt="Age Connects Cardiff & the Vale" />
            break;
        case (int)Groups.MeadowsCommunityHelpers:
            <span class="composite-logo">
                <img src="~/img/logos/HelpMyStreet_BrandMarque_Transparent.svg" alt="HelpMyStreet" />
                <span>Meadows</span>
            </span>
            break;
    }
</div>
