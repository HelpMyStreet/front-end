@model HelpMyStreetFE.Models.Account.Report.ReportViewModel

<style>
    .rTable {
        display: table;
        width: 100%;
    }

    .rTableRow {
        display: table-row;
    }

    .rTableHeading {
        display: table-header-group;
        background-color: #ddd;
    }

    .rTableCell, .rTableHead {
        display: table-cell;
        padding: 3px 10px;
        border: 1px solid #999999;
    }

    .rTableHeading {
        display: table-header-group;
        background-color: #ddd;
        font-weight: bold;
    }

    .rTableFoot {
        display: table-footer-group;
        font-weight: bold;
        background-color: #ddd;
    }

    .rTableBody {
        display: table-row-group;
    }
</style>

<div>
    <div class="chart-container"
         data-chart="@Model.Chart"
         data-group="@Model.GroupId"
         data-type="@Model.ChartType"
         data-datefrom="@Model.DateFrom"
         data-dateto="@Model.DateTo">

        <div class="test"</div>

        <div class="rTable">
            <div class="rTableRow">
                <div class="rTableHead"><strong>Type</strong></div>
                <div class="rTableHead"><span style="font-weight: bold;">Count</span></div>
            </div>
            <div class="rTableRow">
                <div class="rTableCell">Admin</div>
                <div class="rTableCell">3</div>
            </div>
            <div class="rTableRow">
                <div class="rTableCell">Volunteer</div>
                <div class="rTableCell">1410</div>
            </div>
        </div>

    </div>
</div>

